{
  "name": "tripfinder",
  "packageManager": "yarn@1.22.22",
  "description": "react next template",
  "version": "5.0.0",
  "private": true,
  "author": "RedQ, Inc",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "dev": "turbo run dev",
    "dev:boat": "turbo run dev --filter=@tripfinder/boat",
    "build": "turbo run build",
    "build:boat": "turbo run build --filter=@tripfinder/boat",
    "start": "turbo run start",
    "start:boat": "turbo run start --filter=@tripfinder/boat",
    "lint": "turbo run lint",
    "lint:boat": "turbo run lint --filter=@tripfinder/boat",
    "format": "prettier --write .",
    "format:boat": "turbo run format --filter=@tripfinder/boat",
    "check-format": "prettier --check .",
    "clean": "turbo run clean --force && rimraf node_modules",
    "clean:build": "turbo run clean:build",
    "prepare": "husky install"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "husky": "^8.0.1",
    "prettier": "^3.3.0",
    "rimraf": "^5.0.0",
    "serve": "^14.0.1",
    "turbo": "^2.3.0"
  },
  "dependencies": {}
}
